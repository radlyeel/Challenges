%{
//Ref: https://www.rfc-editor.org/rfc/rfc8259.txt

// This header will be generated by Bison
# include "fb1-5.tab.h"
%}
//number = [ minus ] int [ frac ] [ exp ]
// exp = e [ minus / plus ] 1*DIGIT
// frac = decimal-point 1*DIGIT
%%  
"."       { return DOT; }
[1-9]     { return digit1_9;}
[eE]      { return 'e'; }
"{"       { return begin_object; }
"}"       { return end_object; }
:         { return name_separator; }
,         { return value_separator; }
%%
